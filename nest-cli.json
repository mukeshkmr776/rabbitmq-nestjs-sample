{
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/app/src",
  "monorepo": true,
  "root": "apps/app",
  "compilerOptions": {
    "webpack": true,
    "tsConfigPath": "apps/app/tsconfig.app.json"
  },
  "projects": {
    "app": {
      "type": "application",
      "root": "apps/app",
      "entryFile": "main",
      "sourceRoot": "apps/app/src",
      "compilerOptions": {
        "tsConfigPath": "apps/app/tsconfig.app.json"
      }
    },
    "subscriber": {
      "type": "application",
      "root": "apps/subscriber",
      "entryFile": "main",
      "sourceRoot": "apps/subscriber/src",
      "compilerOptions": {
        "tsConfigPath": "apps/subscriber/tsconfig.app.json"
      }
    }
  }
}